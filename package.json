{
  "name": "scdo.js",
  "version": "0.24.1",
  "description": "Generic script api library for Scdo blockchain",
  "main": "./index.js",
  "scripts": {
    "build": "browserify src/scdo.js -r ./src/scdo.js:scdo -t [ babelify --global --presets [ @babel/preset-env ] --plugins [ @babel/plugin-transform-modules-commonjs ] ] -o dist/scdo_browserify.js",
    "debug": "browserify src/scdo.js -r ./src/scdo.js:scdo -t [ babelify --global --presets [ @babel/preset-env ] --plugins [ @babel/plugin-transform-modules-commonjs ] ] --debug -o dist/scdo_browserify_debug.js",
    "esm": "rollup -c",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scdoproject/scdojs.git"
  },
  "keywords": [
    "scdo",
    "javascript",
    "rpc-api",
    "meteor"
  ],
  "author": "scdo@scdoproject.org",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scdoproject/scdojs/issues"
  },
  "homepage": "https://github.com/scdoproject/scdojs#readme",
  "dependencies": {
    "@ethersproject/abi": "^5.7.0",
    "bignumber.js": "^8.0.1",
    "ethers": "^6.13.1",
    "injectpromise": "^1.0.0",
    "json-bignumber": "^1.1.1",
    "keccak": "^1.4.0",
    "lib": "^3.0.2",
    "rlp": "^2.1.0",
    "scrypt-js": "^2.0.4",
    "secp256k1": "^3.5.2",
    "utf8": "^3.0.0",
    "validator": "^13.12.0",
    "xhr2-cookies": "^1.1.0",
    "xmlhttprequest": "^1.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.8",
    "@babel/preset-env": "^7.24.8",
    "@rollup/plugin-replace": "^5.0.7",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0"
  }
}
